#include <stdio.h>
int main()
 {
    int a, b, temp;
    printf("Enter value of a: ");
    scanf("%d", &a);
    printf("Enter value of b: ");
    scanf("%d", &b);
    printf("Original values: a = %d, b = %d\n", a, b);
    temp = a;
    a = b;
    b = temp;
    printf("Swapped values: a = %d, b = %d\n", a, b);
    return 0;
}
OUTPUT:
Enter value of a: 4
Enter value of b: 5
Original values: a = 4, b = 5
Swapped values: a = 5, b = 4
